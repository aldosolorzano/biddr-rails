<h1>Sign up</h1>
<p><%= @user.errors.full_messages.join(', ')%></p>
<%= form_for @user do |f| %>
  <div class="">
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>
  <div class="">
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>
  <div class="">
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div class="">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div class="">
    <%= f.label :password_confirm %>
    <%= f.password_field :password_confirmation %>
  </div>
  <%= f.submit 'Sign Up' %>
<%end%>
